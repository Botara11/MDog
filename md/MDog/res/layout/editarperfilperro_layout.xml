                            android:id="@+id/LinearLayoutanti"

                            android:layout_width="213dp"

                            android:layout_height="wrap_content"

                            android:layout_alignParentLeft="true"

                            android:layout_alignParentRight="true"

                            android:layout_below="@+id/EditTextanti"

                            android:layout_marginLeft="10dp"

                            android:layout_marginRight="10dp"

                            android:layout_marginTop="4dp"

                            android:background="@drawable/linearregistro"

                            android:orientation="vertical"

                            android:padding="10dp" >



                            <EditText

                                android:id="@+id/EditTextfechaproxanti"

                                android:layout_width="match_parent"

                                android:layout_height="wrap_content"

                                android:background="#00000000"

                                android:hint="15/10/2014"

                                android:inputType="date"
                                android:textAppearance="?android:attr/textAppearanceSmall" />

                        </LinearLayout>

                        <EditText
                            android:id="@+id/EditTextultvac"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="17dp"
                            android:background="#00000000"
                            android:ems="10"
                            android:text="Última vacuna"
                            android:textAppearance="?android:attr/textAppearanceSmall" />

                        <EditText

                            android:id="@+id/EditTextchip"

                            android:layout_width="wrap_content"

                            android:layout_height="wrap_content"

                            android:layout_alignLeft="@+id/LinearLayoutanti"

                            android:layout_below="@+id/LinearLayoutanti"

                            android:layout_marginTop="15dp"

                            android:background="#00000000"

                            android:ems="10"

                            android:text="Número de chip"
